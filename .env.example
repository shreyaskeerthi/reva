# Demo mode - set to 1 to use mock/demo responses for all integrations
DEMO_MODE=0

# AWS Configuration
# Region for Bedrock and S3
AWS_REGION=us-east-1
# Optional: AWS profile name from ~/.aws/credentials
AWS_PROFILE=
# S3 bucket for storing run artifacts and evidence
S3_BUCKET=
# Set to 1 to use AWS Bedrock for deal extraction, 0 for heuristic fallback
USE_BEDROCK=1

# Deepgram Speech-to-Text
# Get your API key from https://console.deepgram.com/
DEEPGRAM_API_KEY=

# Merge CRM Integration
# Get your API key from https://app.merge.dev/
MERGE_API_KEY=
# Base URL for Merge CRM API (usually no need to change)
MERGE_BASE_URL=https://api.merge.dev/api/crm/v1
# Your Merge Account Token (X-Account-Token header)
MERGE_ACCOUNT_TOKEN=
